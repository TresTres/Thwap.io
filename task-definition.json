{
    "family": "thwap-fargate",
    "networkMode": "awsvpc",
    "executionRoleArn": "arn:aws:iam::555989344246:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "thwap-container",
            "image": "555989344246.dkr.ecr.us-east-2.amazonaws.com/thwap-image",
            "portMappings": [
                {
                    "containerPort": 3000, 
                    "hostPort": 3000,
                    "protocol": "tcp"
                }
            ],
            "essential": true
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
}